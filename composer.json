{
	"name" : "innovatif/i19n-editor",
	"description" : "i18n translation editor for Silverstripe",
	"type" : "silverstripe-vendormodule",
	"keywords" : [
		"silverstripe",
		"i18n",
        "translation"
	],
	"license" : "BSD-3-Clause",
	"authors" : [
        {
			"name" : "Klemen Dolinsek",
			"email" : "klemen.dolinsek@innovatif.com",
			"role" : "Developer"
		}
	],
	"require" : {
		"silverstripe/framework" : "^5",
		"silverstripe-terraformers/gridfield-rich-filter-header": "^3",
        "tractorcow/silverstripe-fluent": "^7"
	},
    "autoload": {
        "psr-4": {
            "Innovatif\\i19n\\": "src"
        }
    },
    "extra" : {
        "expose" : [
            "css",
            "javascript"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
